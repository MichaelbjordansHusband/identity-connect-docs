{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "generate":  {
      "outputs": ["src/generated/**"]
    },
    "build": {
      "dependsOn": ["^build", "generate"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**", "build/**", "target/**", "ios/**", "android/**"]
    },
    "export": {
      "outputs": ["out/**"],
      "dependsOn": ["build"]
    },
    "lint": {},
    "lint:fix": {},
    "test": {
      "dependsOn": ["build", "^test"],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}
